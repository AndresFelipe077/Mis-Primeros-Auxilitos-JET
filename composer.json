{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.5",
        "doctrine/inflector": "^2.0.5",
        "doctrine/lexer": "2.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jeroennoten/laravel-adminlte": "^3.8",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.12",
        "laravel/passport": "^11.4",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.4",
        "laraveles/spanish": "^1.5",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mercadopago/dx-php": "^3.0",
        "mercadopago/sdk": "^0.5.6",
        "paypal/rest-api-sdk-php": "^1.6",
        "pragmarx/google2fa-laravel": "^2.0",
        "spatie/laravel-permission": "^5.7",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "jn-jairo/laravel-ngrok": "^2.0",
        "kitloong/laravel-migrations-generator": "^6.7",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nascent-africa/jetstrap": "^2.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "MercadoPago\\": "path/to/sdk/folder"
            
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
